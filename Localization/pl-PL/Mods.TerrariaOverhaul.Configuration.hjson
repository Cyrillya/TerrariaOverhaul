	}

	LowManaSoundVolume: {
		DisplayName: "Głośność dźwięku niskiego poziomu many"
		Description: "Kontroluje głośność dźwięku który ma miejsce przy niskim poziomie many."
	}

	ManaRegenerationSoundVolume: {
		DisplayName: "Głośność dźwięku regeneracji many"
		Description: "Kontroluje głośność dźwięku który ma miejsce gdy mana się regeneruje."
	}
}

